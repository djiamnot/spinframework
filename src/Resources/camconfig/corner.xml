<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE soundEngine SYSTEM>
<camConfig>
    <window id="cornerWindow">
        <screen>0</screen>
        <fullscreen>false</fullscreen>
        <windowPosition>50 50</windowPosition>
        <windowSize>400 400</windowSize>
        <clearColor>0 0 0 0</clearColor>
        <windowDecoration>false</windowDecoration>
        <useCursor>true</useCursor>
        <supportsResize>true</supportsResize>
        <camera id="forwardCamera">
            <parent>world</parent>
            <eye>0 0 0</eye>
            <lookat>0 1 0</lookat>
            <up>0 0 1</up>
            <viewport>0 0 1 1</viewport>
        </camera>
    </window>
    <window id="rightWindow">
        <screen>0</screen>
        <fullscreen>false</fullscreen>
        <windowPosition>450 50</windowPosition>
        <windowSize>400 400</windowSize>
        <clearColor>1 0 0 0</clearColor>
        <windowDecoration>false</windowDecoration>
        <useCursor>true</useCursor>
        <supportsResize>true</supportsResize>
        <camera id="rightCamera">
            <parent>world</parent>
            <eye>0 0 0</eye>
            <lookat>1 0 0</lookat>
            <up>0 0 1</up>
            <viewport>0 0 1 1</viewport>
        </camera>
    </window>
</camConfig>
