<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="wxVessEditor">
		<title>SPIN :: Editor</title>
		<size>-1,600</size>
		<id_arg>0</id_arg>
		<object class="wxSplitterWindow" name="ID_SPLITTERWINDOW1" variable="vessEditor_splitter" member="yes">
			<sashpos>150</sashpos>
			<minsize>50</minsize>
			<minsize>50,50</minsize>
			<object class="wxPanel" name="ID_treePanel" variable="treePanel" member="yes">
				<object class="wxBoxSizer" variable="BoxSizer1" member="no">
					<orient>wxVERTICAL</orient>
					<object class="sizeritem">
						<object class="Custom" name="ID_VESS_TREE" subclass="wxVessTreeCtrl" variable="vessTree" member="yes">
							<creating_code>$(THIS) = new $(CLASS)($(PARENT),$(ID),$(POS),$(SIZE),$(STYLE),wxDefaultValidator,$(NAME));</creating_code>
							<include_file>wxVessTreeCtrl.h</include_file>
							<local_include>1</local_include>
							<style>wxTR__DEFAULT__STYLE</style>
						</object>
						<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
			</object>
			<object class="wxPanel" name="ID_PANEL2" variable="editorPanel" member="yes">
				<object class="wxStaticBoxSizer" variable="StaticBoxSizer1" member="no">
					<label>Editor:</label>
					<object class="sizeritem">
						<object class="Custom" name="ID_CUSTOM1" subclass="wxVessPropGrid" variable="VessPropGrid" member="yes">
							<creating_code>$(THIS) = new $(CLASS)($(PARENT),$(ID),$(POS),$(SIZE));</creating_code>
							<include_file>wxVessPropGrid.h</include_file>
							<local_include>1</local_include>
							<style></style>
						</object>
						<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
			</object>
		</object>
		<object class="wxToolBar" name="ID_TOOLBAR1" variable="wxVessEditor_ToolBar" member="yes">
			<bitmapsize>-1,32</bitmapsize>
			<object class="tool" name="vessEditor_clear" variable="ToolBarItem1" member="yes">
				<label>Clear</label>
				<bitmap>../images/icon_eraser.gif</bitmap>
				<bitmap2>../images/icon_eraser.gif</bitmap2>
				<tooltip>Clear the current scene</tooltip>
				<longhelp>Clear the current scene</longhelp>
				<handler function="OnClear" entry="EVT_TOOL" />
			</object>
			<object class="tool" name="vessEditor_refresh" variable="ToolBarItem2" member="yes">
				<label>Refresh</label>
				<bitmap>../images/icon_refresh.gif</bitmap>
				<bitmap2>../images/icon_refresh.gif</bitmap2>
				<tooltip>Refresh the scene</tooltip>
				<longhelp>This will resync with the VESS server, ensuring that the scene is up to date.</longhelp>
				<handler function="OnRefresh" entry="EVT_TOOL" />
			</object>
			<object class="separator" />
			<object class="tool" name="vessEditor_debugPrint" variable="ToolBarItem3" member="yes">
				<label>DebugPrint</label>
				<bitmap>../images/icon_question_text.gif</bitmap>
				<bitmap2>../images/icon_question_text.gif</bitmap2>
				<tooltip>Debug Print to Console</tooltip>
				<longhelp>Debug Print to Console</longhelp>
				<handler function="OnDebugPrint" entry="EVT_TOOL" />
			</object>
			<object class="separator" />
			<object class="tool" name="vessEditor_newNode" variable="ToolBarItem4" member="yes">
				<label>New Node</label>
				<bitmap stock_id="wxART_NEW" stock_client="wxART_BUTTON" />
				<bitmap2 stock_id="wxART_NEW" stock_client="wxART_BUTTON" />
				<tooltip>Create a new node</tooltip>
				<longhelp>This will allow you to create a new node (a dialog will let you choose the type)</longhelp>
				<handler function="OnNewNode" entry="EVT_TOOL" />
			</object>
			<object class="tool" name="vessEditor_deleteNode" variable="ToolBarItem5" member="yes">
				<label>Delete Selected Node</label>
				<bitmap stock_id="wxART_DELETE" stock_client="wxART_BUTTON" />
				<bitmap2 stock_id="wxART_DELETE" stock_client="wxART_BUTTON" />
				<tooltip>Delete the currently selected node</tooltip>
				<longhelp>Delete the currently selected node</longhelp>
				<handler function="OnDeleteNode" entry="EVT_TOOL" />
			</object>
		</object>
	</object>
</wxsmith>
